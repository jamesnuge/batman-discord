{
    "name": "@discord/batman",
    "version": "0.0.1",
    "keywords": [
        "discord",
        "loveletter",
        "batman"
    ],
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "test": "karma start --reporters mocha",
        "test-ci": "karma start --single-run --reporters teamcity",
