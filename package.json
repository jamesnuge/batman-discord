        "watch-build": "rimraf dist && webpack --config .config/webpack.prod-build.js --progress --profile --bail --watch"
    },
    "dependencies": {
        "discord.js": "12.1.1"
    },
    "devDependencies": {
        "@5b1/common-web": "^0.1.119"
    }
}
